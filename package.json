    "clownface": "^1.0.0",
    "content-type": "^1.0.4",
    "http-status-codes": "^1.4.0",
    "jsonld": "^3.1.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-static": "^5.0.0",
    "mongodb": "3.6.0",
    "p-event": "^4.2.0",
